{
  "rules": {
    "no-extra-parens": 2,
    "valid-jsdoc": 2,
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "complexity": 2,
    "curly": 2,
    "dot-location": 2,
    "dot-notation": 2,
    "no-div-regex": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-param-reassign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 2,
    "no-useless-concat": 2,
    "vars-on-top": 0,
    "wrap-iife": 2,
    "yoda": 2,
    "strict": [
      2,
      "global"
    ],
    "init-declarations": 2,
    "no-catch-shadow": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-use-before-define": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": 2,
    "no-restricted-imports": 2,
    "no-restricted-modules": 2,
    "no-sync": 2,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "eol-last": 2,
    "func-names": 0,
    "func-style": 2,
    "id-blacklist": 2,
    "id-length": 2,
    "id-match": 2,
    "indent": [
      2,
      2,
      {
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        }
      }
    ],
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "max-depth": 2,
    "max-len": 0,
    "max-nested-callbacks": 2,
    "max-params": 2,
    "max-statements": 2,
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "newline-per-chained-call": 2,
    "no-bitwise": 2,
    "no-inline-comments": 0,
    "no-multiple-empty-lines": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-restricted-syntax": 2,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": 2,
    "one-var": [
      2,
      {
        "uninitialized": "always"
      }
    ],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "quote-props": 2,
    "quotes": 2,
    "require-jsdoc": 2,
    "semi": 2,
    "semi-spacing": 2,
    "sort-imports": 2,
    "sort-vars": 0,
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "never",
        "named": "never"
      }
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "wrap-regex": 2,
    "arrow-body-style": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "generator-star-spacing": 2,
    "no-confusing-arrow": 2,
    "no-useless-constructor": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-reflect": 2,
    "prefer-template": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": false,
    "node": true,
    "mocha": true
  }
}